public boolean isAnagram(String s1, String s2) {
    List<String> s1List = s1.split('');
    List<String> s2List = s2.split('');

    Boolean isAnagram = true;
    for(String s : s1List){
        if(!s2List.contains(s)){
            isAnagram = false;
            break;
        }
    }
    return isAnagram;
}