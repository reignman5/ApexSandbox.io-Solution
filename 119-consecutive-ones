public Integer maxConsecutiveOnes(Integer[] numbers) {
    Integer maxCount = 0;
    for(Integer i = 0; i<numbers.size();i++){
        Integer tempCount = 0;
        if(numbers[i]==1){
            tempCount++;
        } else{
            if(tempCount > maxCount){
                maxCount = tempCount;
            }
            tempCount = 0;
        }       
    }
    return maxCount;
}